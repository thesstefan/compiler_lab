int n;
read(n);
char[] prime_message = "Number is prime"
char[] not_prime_message = "Number is NOT prime"
if (n == 2 || n == 3) {
write(prime_message);
return;
}
if (n <= 1 || n % 2 == 0 || n % 3 == 0) {
write(not_prime_message);
return;
}
for (int i = 5; i * i <= n; i += 6) {
if (n % i == 0 || n % (i + 2) == 0) {
write(not_prime_message);
return;
}
}
write(prime_message);
